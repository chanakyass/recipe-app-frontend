{
  "name": "recipe-app-frontend",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "bootstrap": "^4.6.0",
    "firebase": "^8.7.1",
    "history": "^4.10.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "msw": "^1.3.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-cookies": "^0.1.1",
    "react-dom": "^17.0.2",
    "react-redux": "^8.1.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.9.5",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "react-scripts test --watchAll=false --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-cookies": "^0.1.0",
    "@types/react-dom": "^18.2.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.4",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.ts",
      "**/*.tsx",
      "!**/node_modules/**",
      "!**/src/**/index.ts",
      "!**/src/**/*.model.ts",
      "!**/src/**/setup.ts",
      "!**/src/tests/**/*",
      "!**/react-app-env.d.ts"
    ],
    "coverageThreshold": {
      "global": {
          "functions": 50,
          "branches": 40,
          "lines": 50,
          "statements": 50
      }
    },
    "testMatch": [
      "**/*.test.ts",
      "**/*.test.tsx"
    ]
  }
}
